resource() {
  source ${HOME}/.zshrc
}

fin() {
  say done
  open raycast://confetti
}

gu() {
  branch="$1"
  git checkout "$branch"
  git pull
  git checkout -
}

gch() {
 git checkout "$(git branch | fzf | tr -d '[:space:]')"
}

function gs() {
  selected=$(git diff --name-only --diff-filter=ACDMRTUXB | fzf --multi)
  if [ -n "$selected" ]; then
    git add $(echo $selected | xargs)
  fi
}

function t {
  mkdir -p $(dirname $1)
  touch $1
}
